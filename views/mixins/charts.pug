mixin charts
  .col-sm-12
    .panel.panel-default
      .panel-body
        ul.nav.nav-tabs
          li.active(role="presentation")
            a(href="#bar-chart" data-toggle="tab")
              i.fa.fa-bar-chart
              |  Revision number distribution by year and by user type
          li(role="presentation" id="pie-chart-tab-btn")
            a(href="#pie-chart" data-toggle="tab")
              i.fa.fa-pie-chart
              |  Revision number distribution by user type

        .tab-content.clearfix
          #bar-chart.tab-pane.active
            #bar-chart-1


          #pie-chart.tab-pane
            #pie-chart-1
